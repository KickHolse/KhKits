spawn:
  warps:
    slot: 2
    execute: 'menu>warps'
    icon: 'COMPASS : nome>&eWarps'
  kits:
    slot: 3
    execute: 'menu>kits'
    icon: 'NETHER_STAR : nome>&eKits'
  shop:
    slot: 5
    execute: 'menu>shop'
    icon: 'EMERALD : nome>&eShop'
  moneyday:
    slot: 6
    execute: 'menu>moneyday'
    icon: 'CHEST_MINECART : 1 : nome>&aDaily Coins'
    icon-i: 'MINECART : 1 : nome>&cDaily Coins'

ingame:
  # the first number is the slot of item
  soup: 'SOUP : 36 : nome>&aSoup'
